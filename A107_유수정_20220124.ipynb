    "#https://www.acmicpc.net/problem/1292\n",
    "a, b = map(int, input().split())\n",
    "l = [0 for i in range(b)]\n",
    "cnt = 0\n",
    "n = 1\n",
    "ans = 0\n",
    "for i in range(b):\n",
    "    l[i] = n\n",
    "    cnt += 1\n",
    "    if n == cnt:\n",
    "        n += 1\n",
    "        cnt = 0\n",
    "for i in range(a-1, b):\n",
    "    ans += l[i]\n",
    "print(ans)"
   ]
