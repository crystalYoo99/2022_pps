   "source": [
    "n = int(input())\n",
    "l = list(map(int, input().split()))\n",
    "l.sort()\n",
    "max_l = 0\n",
    "\n",
    "for i in range(n):\n",
    "    l[i] = l[i] - i\n",
    "    if l[i] > max_l:\n",
    "        max_l = l[i]\n",
    "\n",
    "print(max_l + n + 1)"
   ]
