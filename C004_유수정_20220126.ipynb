    "#https://www.acmicpc.net/problem/2231\n",
    "n = int(input())\n",
    "\n",
    "for i in range(1, n+1):\n",
    "    n_num = sum(list(map(int, str(i)))) + i\n",
    "    if n_num == n:\n",
    "        print(i)\n",
    "        break\n",
    "    if i == n:\n",
    "        print(0)\n"
