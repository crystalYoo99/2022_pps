    "#https://www.acmicpc.net/problem/2217\n",
    "n = int(input())\n",
    "\n",
    "rope = []\n",
    "for i in range(n):\n",
    "    rope.append(int(input()))\n",
    "\n",
    "\n",
    "rope.sort(reverse = True)\n",
    "\n",
    "max = 0\n",
    "for i in range(n):\n",
    "    temp = rope[i]*(i+1)\n",
    "    if temp > max:\n",
    "        max = temp\n",
    "\n",
    "print(max)"
   ]
